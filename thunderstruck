/*************************************************
 * Melodies and Note Duration Values
 *************************************************/

 #include "pitches.h"

//Used to store the melodies on the Flash Memory (Program Storage) rather than in the dynamic memory
 #include <avr/pgmspace.h>

 //PROGMEM needs to be in front of the melody and note durations arrays in order to
 //save these (very) large arrays in the Flash Memory

 //Duration/BPM for notes
 #define WHOLE_NOTE_PER_BPM 240 // 240/BPM
 #define HALF_NOTE_PER_BPM 120 // 120/BPM
 #define QUARTER_NOTE_PER_BPM 60 // 60/BPM
 #define EIGHT_NOTE_PER_BPM 30 // 30/BPM
 #define SIXTEENTH_NOTE_PER_BPM 15 // 15/BPM
 #define DOTTED_QUARTER_NOTE_PER_BPM 90 // 15/BPM
 #define DOTTED_EIGHT_NOTE_PER_BPM 45 // 45/BPM
 #define DOTTED_SIXTEENTH_NOTE_PER_BPM 22.5 // 22.5/BPM
 #define TRIPLET_QUARTER_NOTE_PER_BPM 40 // 40/BPM
 #define TRIPLET_EIGHT_NOTE_PER_BPM 20 // 20/BPM
 #define TRIPLET_SIXTEENTH_NOTE_PER_BPM 10 // 20/BPM

 //Melodies and note durations

